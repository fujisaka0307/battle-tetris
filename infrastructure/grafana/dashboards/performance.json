  ],
  "uid": "battle-tetris-perf",
  "title": "Battle Tetris - Performance",
  "description": "Server and client performance metrics for the Battle Tetris game",
  "tags": ["battle-tetris", "performance"],
  "editable": true,
  "refresh": "30s",
  "schemaVersion": 39,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "browser",
  "panels": [
    {
      "type": "row",
      "title": "Server HTTP",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "Request Latency p50 / p95 / p99",
      "description": "HTTP request duration percentiles over a 5-minute window",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 1 },
      "id": 2,
      "datasource": {
        "type": "prometheus",
        "uid": "grafanacloud-prom"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.5, rate(http_server_request_duration_seconds_bucket[5m]))",
          "legendFormat": "p50",
          "editorMode": "code"
        },
        {
          "refId": "B",
          "expr": "histogram_quantile(0.95, rate(http_server_request_duration_seconds_bucket[5m]))",
          "legendFormat": "p95",
          "editorMode": "code"
        },
        {
          "refId": "C",
          "expr": "histogram_quantile(0.99, rate(http_server_request_duration_seconds_bucket[5m]))",
          "legendFormat": "p99",
          "editorMode": "code"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "insertNulls": false,
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "never",
            "spanNulls": false,
            "stacking": { "group": "A", "mode": "none" },
            "thresholdsStyle": { "mode": "off" }
          },
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.5 },
              { "color": "red", "value": 1 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    },
    {
      "type": "timeseries",
      "title": "Request Rate",
      "description": "Total HTTP requests per second (5-minute rate)",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 1 },
      "id": 3,
      "datasource": {
        "type": "prometheus",
        "uid": "grafanacloud-prom"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(http_server_request_duration_seconds_count[5m]))",
          "legendFormat": "req/s",
          "editorMode": "code"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 20,
            "gradientMode": "scheme",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "insertNulls": false,
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "never",
            "spanNulls": false,
            "stacking": { "group": "A", "mode": "none" },
            "thresholdsStyle": { "mode": "off" }
          },
          "unit": "reqps",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    },
    {
      "type": "timeseries",
      "title": "Error Rate",
